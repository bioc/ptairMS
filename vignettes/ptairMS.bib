	file = {Miekisch et al. - 2004 - Diagnostic potential of breath analysis—focus on v.pdf:C\:\\Users\\CR258086\\Zotero\\storage\\YYM7NC77\\Miekisch et al. - 2004 - Diagnostic potential of breath analysis—focus on v.pdf:application/pdf}
}

