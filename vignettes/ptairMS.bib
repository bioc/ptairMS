	pages = {046009},
	file = {Cristescu et al. - 2011 - Screening for emphysema via exhaled volatile organ.pdf:C\:\\Users\\CR258086\\Zotero\\storage\\IMZKI4WX\\Cristescu et al. - 2011 - Screening for emphysema via exhaled volatile organ.pdf:application/pdf}
