	pages = {60--63},
	file = {Cappellin et al. - 2010 - Improved mass accuracy in PTR-TOF-MS Another step.pdf:C\:\\Users\\CR258086\\Zotero\\storage\\H9M9JVFV\\Cappellin et al. - 2010 - Improved mass accuracy in PTR-TOF-MS Another step.pdf:application/pdf}
}

