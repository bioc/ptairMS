	pages = {868--875},
	file = {Christenfeld et al. - 2004 - Risk Factors, Confounding, and the Illusion of Sta.pdf:C\:\\Users\\CR258086\\Zotero\\storage\\RCMXB8JU\\Christenfeld et al. - 2004 - Risk Factors, Confounding, and the Illusion of Sta.pdf:application/pdf}
}

