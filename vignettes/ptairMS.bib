	pages = {72--77},
	file = {Titzmann et al. - 2010 - Improved peak analysis of signals based on countin.pdf:C\:\\Users\\CR258086\\Zotero\\storage\\WFQ6E2DM\\Titzmann et al. - 2010 - Improved peak analysis of signals based on countin.pdf:application/pdf}
}

