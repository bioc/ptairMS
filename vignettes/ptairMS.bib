	file = {Trefz et al. - 2018 - Effects of humidity, CO sub2sub and O sub2.pdf:C\:\\Users\\CR258086\\Zotero\\storage\\UEV8KV3S\\Trefz et al. - 2018 - Effects of humidity, CO sub2sub and O sub2.pdf:application/pdf}
