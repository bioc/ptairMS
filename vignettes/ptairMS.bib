	urldate = {2019-01-31},
	journal = {Chemometrics and Intelligent Laboratory Systems},
	author = {Cappellin, Luca and Aprea, Eugenio and Granitto, Pablo and Wehrens, Ron and Soukoulis, Christos and Viola, Roberto and Märk, Tilmann D. and Gasperi, Flavia and Biasioli, Franco},
