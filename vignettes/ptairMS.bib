	urldate = {2020-07-29},
	journal = {Journal of Breath Research},
	author = {Malásková, Michaela and Henderson, Ben and Chellayah, Prema D and Ruzsanyi, Veronika and Mochalski, Paweł and Cristescu, Simona M and Mayhew, Chris A},
	month = jul,
