	author = {Smith, David and Španěl, Patrik and Herbig, Jens and Beauchamp, Jonathan},
	month = mar,
	year = {2014},
	pages = {027101},
	file = {Smith et al. - 2014 - Mass spectrometry for real-time quantitative breat.pdf:C\:\\Users\\CR258086\\Zotero\\storage\\ZWAMC2FZ\\Smith et al. - 2014 - Mass spectrometry for real-time quantitative breat.pdf:application/pdf}
